<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.17.xsd">

    <changeSet id="3-create-TB_DADOS_COVID-table" author="usuario">
        <createTable tableName="TB_USERS">
            <column name="ID" type="NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY">
                <constraints nullable="false"/>
            </column>
            <column name="NAME" type="VARCHAR2(60)"/>
            <column name="EMAIL" type="VARCHAR2(60)"/>
            <column name="CPF" type="NUMBER"/>
            <column name="CREATED_AT" type="DATE"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
